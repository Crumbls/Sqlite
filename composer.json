{
    "name": "crumbls/sqlite",
    "description": "A Laravel Filament SQLite admin tool",
    "type": "library",
    "license": "MIT",
    "version": "0.1.0-beta.1",
    "autoload": {
        "psr-4": {
            "Crumbls\\Sqlite\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Crumbls",
            "email": "developer@crumbls.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "calebporzio/sushi": "^2.5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Crumbls\\Sqlite\\SqliteServiceProvider"
            ]
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true
}
